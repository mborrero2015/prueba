
SELECT LABCULTI.Numcontrol,LABCULTI.Codlabor,LABCULTI.Nomlabor,;
    LABCULTI.Observa,JORCULTI.Numcontrol,JORCULTI.Codlabor,;
    JORCULTI.Nomlabor,JORCULTI.Observa;
    FROM SACFADB!LABCULTI LABCULTI FULL OUTER JOIN SACFADB!JORCULTI JORCULTI ON LABCULTI.NUMCONTROL = JORCULTI.NUMCONTROL;
    ORDER BY LABCULTI.Codlabor,JORCULTI.Codlabor